    "eslint": "^9.25.1",
    "eslint-config-expo": "^9.2.0",
    "eslint-config-prettier": "^10.1.2",
    "nativewind": "^2.0.11",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "react-native-css-transformer": "^2.0.0",
    "react-native-dotenv": "^3.4.11",

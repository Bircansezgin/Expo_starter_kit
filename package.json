    "@react-native-picker/picker": "^2.11.1",
    "@react-navigation/bottom-tabs": "^7.3.14",
    "@react-navigation/native": "^7.1.10",
    "@react-navigation/native-stack": "^7.3.14",
    "@types/i18n": "^0.13.12",
    "axios": "^1.9.0",
    "expo": "^53.0.11",
    "expo-audio": "~0.4.8",
    "expo-av": "~15.1.7",
    "expo-blur": "~14.1.5",
    "expo-clipboard": "~7.1.4",
    "expo-constants": "^17.1.7",
    "expo-device": "~7.1.4",

    "expo-localization": "^16.1.5",
    "expo-notifications": "^0.31.3",
    "expo-status-bar": "~2.2.3",
    "i18n-js": "^4.5.1",
    "moment": "^2.30.1",
    "react": "19.0.0",
    "react-i18next": "^15.5.2",
    "react-native": "0.79.3",
